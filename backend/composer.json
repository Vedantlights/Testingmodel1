{
    "name": "indiapropertys/backend",
    "description": "India Propertys Backend API",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "google/cloud-vision": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\": "services/",
            "App\\Utils\\": "utils/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
